<data>
    <xpath
        expr="/form/notebook/page[@id='esale']/notebook/page[@id='esale-general']/field[@name='esale_sequence']"
        position="after">
            <label name="magento_product_type"/>
            <field name="magento_product_type"/>
            <label name="magento_group_price"/>
            <field name="magento_group_price"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='esale']/notebook/page[@id='esale-general']"
        position="after">
            <page string="Attribute Configurables" col="4" id="magento-attribute-configurables">
