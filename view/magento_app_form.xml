<data>
    <xpath
        expr="/form/notebook/page[@id='import']/button[@name='core_customer_group']"
        position="after">
            <separator string="Products" colspan="4" id="products"/>
            <button name="core_import_product_type" string="Import Product Type" colspan="4"/>
            <button name="core_import_group_attributes" string="Import Group Attributes" colspan="4"/>
            <group col="6" colspan="4" id="categories">
                <button name="core_import_categories" string="Import Magento Categories"/>
                <label name="category_root_id"/>
                <field name="category_root_id"/>
            </group>
            <group col="6" colspan="4" id="products">
                <button name="core_import_products" string="Import/Update Magento Products"/>
                <label name="from_products"/>
                <field name="from_products"/>
                <label name="to_products"/>
                <field name="to_products"/>
            </group>
            <button name="core_import_images" string="Import Magento Images" colspan="4"/>
    </xpath>
</data>
